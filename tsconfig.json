{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "noImplicitAny": true,
        "module": "commonjs",
        "target": "es5",
        "jsx": "react",
        "baseUrl": "src",
        "moduleResolution": "node",
        "noImplicitAny": false,
        "sourceMap": true,
        "allowSyntheticDefaultImports": true,
	    "lib": ["dom", "es6"]
    },
    "exclude": [
        "node_modules",
        "AOMAnalyzer-darwin-x64",
        "AOMAnalyzer-linux-x64",
        "AOMAnalyzer-win32-x64"
    ]
}
